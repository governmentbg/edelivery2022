<?xml version="1.0" encoding="utf-8" ?>
<parameters>
  <parameter name="appSettings:DomibusRestServiceUser">
    <parameterValidation kind="AllowEmpty" />
    <parameterEntry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/appSettings/add[@key='DomibusRestServiceUser']/@value" />
  </parameter>

  <parameter name="appSettings:DomibusRestServicePassword">
    <parameterValidation kind="AllowEmpty" />
    <parameterEntry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/appSettings/add[@key='DomibusRestServicePassword']/@value" />
  </parameter>
</parameters>
