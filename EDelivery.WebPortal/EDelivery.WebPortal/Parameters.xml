  <parameter name="appSettings:BulSISecretKey">
    <parameterValidation kind="AllowEmpty" />
    <parameterEntry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/location/appSettings/add[@key='BulSISecretKey']/@value" />
  </parameter>

  <parameter name="appSettings:PaySecretKey">
    <parameterValidation kind="AllowEmpty" />
    <parameterEntry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/location/appSettings/add[@key='PaySecretKey']/@value" />
