  <parameter name="appSettings:NOIAuthSharedSecret">
    <parameterValidation kind="AllowEmpty" />
    <parameterEntry
      kind="XmlFile"
      scope="\\web.config$"
      match="/configuration/location/appSettings/add[@key='NOIAuthSharedSecret']/@value" />
  </parameter>
</parameters>
