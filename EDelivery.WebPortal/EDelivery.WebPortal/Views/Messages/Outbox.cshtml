@model OutboxViewModel

@{
    ViewBag.Title = Common.TitleSentMessages;
    ViewBag.SearchSubject = ProfilePage.TableHeaderMessageTitle;
    ViewBag.SearchProfile = ProfilePage.TableHeaderMessageReceiver;

    ViewBag.Legend = ProfilePage.TitleSearchInSentMessages;
}

<div class="cols-wrapper">
    @Html.Partial("_LeftMenu")

    <div class="content-right">
        <h2>@Common.TitleSentMessages</h2>

        @Html.Partial("Partials/_SearchMessages", Model.SearchFilter)

        @Html.Partial("Partials/_OutboxMessageList", Model)
    </div>
</div>
