<log4net xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appender name="GlobalAppender" xdt:Locator="Match(name)">
    <file type="log4net.Util.PatternString" value="%env{SystemDrive}\inetpub\logs\egovtestv2-edelivery-webportal\Global.txt" xdt:Transform="SetAttributes(type,value)" />
  </appender>
  <root>
    <level value="ALL" xdt:Transform="Replace" />
  </root>
  <logger name="Global" xdt:Locator="Match(name)" xdt:Transform="Remove" />
</log4net>