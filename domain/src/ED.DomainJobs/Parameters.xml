<?xml version="1.0" encoding="utf-8" ?>
<parameters>
  <parameter name="Environment">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ASPNETCORE_ENVIRONMENT']/@value"
    />
  </parameter>

  <parameter name="LogPath">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/@stdoutLogFile"
    />
  </parameter>

  <parameter name="InfosystemsApiUserName">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ED__Clients__Infosystems__ApiUserName']/@value"
      defaultValue=""
    />
  </parameter>

  <parameter name="InfosystemsApiPassword">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ED__Clients__Infosystems__ApiPassword']/@value"
      defaultValue=""
    />
  </parameter>

  <parameter name="SharedSecretDPKey">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ED__Authentication__SharedSecretDPKey']/@value"
      defaultValue=""
    />
  </parameter>

  <parameter name="ETranslationUsername">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ED__Clients__ETranslation__Username']/@value"
      defaultValue=""
    />
  </parameter>

  <parameter name="ETranslationPassword">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ED__Clients__ETranslation__Password']/@value"
      defaultValue=""
    />
  </parameter>

  <parameter name="DataPortalApiKey">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ED__Clients__DataPortal__ApiKey']/@value"
      defaultValue=""
    />
  </parameter>
</parameters>
