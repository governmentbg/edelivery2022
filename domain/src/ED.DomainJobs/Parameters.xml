<?xml version="1.0" encoding="utf-8" ?>
<parameters>
  <parameter name="Environment">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ASPNETCORE_ENVIRONMENT']/@value"
    />
  </parameter>

  <parameter name="LogPath">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/@stdoutLogFile"
    />
  </parameter>

<parameter name="LinkMobilityApiKey">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ED__Clients__LinkMobility__ApiKey']/@value"
      defaultValue=""
    />
  </parameter>

<parameter name="LinkMobilityApiSecret">
    <parameterEntry
      kind="XmlFile"
      scope="Web.config"
      match="/configuration/location/system.webServer/aspNetCore/environmentVariables/environmentVariable[@name='ED__Clients__LinkMobility__ApiSecret']/@value"
      defaultValue=""
    />
  </parameter>
</parameters>
