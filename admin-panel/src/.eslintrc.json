{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "plugins": [ "compat" ],
  "rules": {
    "compat/compat": "error"
  },
  "extends": "eslint:recommended",
  "globals": {
    "System": "readonly",
    "$": "readonly"
  }
}
